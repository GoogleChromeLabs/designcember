---
import Pic from '../Pic/Pic.astro';
import Number from '../Calendar/Number.astro';
---

<style>
  .twentynine {
    margin: var(--size-3);

    @media (--md) {
      --_day-column: 2;
    }

    @media (--xl) {
      --_day-column: 2;
    }
  }

  .plaque {
    z-index: var(--layer-1);
    place-self: flex-start center;
    margin-block-start: -10px;
  }

  .frame {
    min-block-size: 200px;
    background: var(--_day-bg);
    border: var(--size-2) solid #d9773e;
  }
</style>

<div id="29th" class="day twentynine stacked" title="">
  <span class="plaque">
    <Number num="29" link="#29th" />
  </span>
  <div class="frame cq-interactive"></div>
</div>